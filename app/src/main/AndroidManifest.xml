<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.adrianedilao.massform">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Massform"
        tools:targetApi="31">
        <activity
            android:name=".AWorkoutActivity"
            android:exported="false"
            android:theme="@style/Theme.Massform.NoActionBar" />
        <activity
            android:name=".BWorkoutActivity"
            android:exported="false"
            android:theme="@style/Theme.Massform.NoActionBar" />
        <activity
            android:name=".WeightConverterActivity"
            android:exported="false"
            android:theme="@style/Theme.Massform.NoActionBar" />
        <activity
            android:name=".MenuActivity"
            android:exported="false"
            android:label="@string/title_activity_menu"
            android:theme="@style/Theme.Massform.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Massform.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>